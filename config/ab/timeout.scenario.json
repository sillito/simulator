{
  "description": "Circuit Breakers, Timeouts, Retries, Queues, Cache, degredation",
  "autoAssign": {
    "port": true
  },
  "services": [
    {
      "name": "B",
      "file": "../config/ab/b.service.json"
    },
    {
      "name": "B - latency 50-10",
      "file": "../config/ab/b.service.json",
      "timedResponseSettings": {
        "mean": 50,
        "std": 10
      }
    },
    {
      "name": "B - latency 80-10",
      "file": "../config/ab/b.service.json",
      "timedResponseSettings": {
        "mean": 80,
        "std": 10
      }
    },
    {
      "name": "B - latency 40-10",
      "file": "../config/ab/b.service.json",
      "timedResponseSettings": {
        "mean": 40,
        "std": 10
      }
    },

    {
      "name": "B - low availability",
      "file": "../config/ab/b.service.json",
      "timedResponseSettings": {
        "failureRate": 0.1
      }
    }
  ],
  "endpoints": [
    {
      "name": "Timeout",
      "file": "../config/ab/a.service.json",
      "timeout": 50
    },
    {
      "name": "50 B - Timeout",
      "file": "../config/ab/a.service.json",
      "dependencies": [
        {
          "name": "B - latency 50-10"
        }
      ],
      "timeout": 50
    },
    {
      "name": "40 B - Timeout",
      "file": "../config/ab/a.service.json",
      "dependencies": [
        {
          "name": "B - latency 40-10"
        }
      ],
      "timeout": 50
    },
    {
      "name": "80 B - Timeout",
      "file": "../config/ab/a.service.json",
      "dependencies": [
        {
          "name": "B - latency 80-10"
        }
      ],
      "timeout": 50
    }
  ]
}
